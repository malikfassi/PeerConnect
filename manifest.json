{
  "manifest_version": 2,

  "name": "42Friends",
  "description": "This extension demonstrates a 'browser action' with kittens.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "https://dashboard.42.fr/crawler/pull/*"
  ],
  "content_security_policy": "script-src 'self' https://dashboard.42.fr/crawler/pull/; object-src 'self'"
}